{
  "name": "atelier-web",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "node server.js",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public/**\"",
    "build": "next build",
    "copy-resources": "cpx \"functions/**/*.*\" \"dist/functions\" && cpx \"public/**/*.*\" \"dist/public\" && cpx \"dist/functions/build/static/**/*.*\" \"dist/public/_next/static\" && cpx \"config/**/*.*\" \"dist/functions/config\"",
    "install-dependencies": "cpx \"*{package.json,package-lock.json}\" \"dist/functions\" && cd \"dist/functions\" && npm install",
    "preserve": "npm run clean && npm run build && npm run copy-resources && npm run install-dependencies",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "prettier": "prettier src/**/*.js --write"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.2",
    "config": "^3.2.5",
    "decimal.js": "^10.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "moment": "^2.24.0",
    "next": "^9.2.1",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "reselect": "^4.0.0",
    "underscore": "^1.9.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "cross-env": "^7.0.0",
    "firebase-tools": "^7.13.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  },
  "prettier": {
    "semi": false,
    "jsxBracketSameLine": true
  }
}
